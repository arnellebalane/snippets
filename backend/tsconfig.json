{
    "compilerOptions": {
        "outDir": "dist",
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "skipLibCheck": true,
        "allowSyntheticDefaultImports": true
    },
    "include": [
        "core/**/*",
        "lambda/**/*",
        "utils/**/*"
    ]
}
